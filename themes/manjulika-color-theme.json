{
  "$schema": "vscode://schemas/color-theme",
  "name": "Manjulika Ember",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "foreground": "#f4e6d1",
    "editor.background": "#0d0907",
    "editor.foreground": "#f4e6d1",
    "editorCursor.foreground": "#e8b44c",
    "editor.lineHighlightBackground": "#19100d",
    "editor.selectionBackground": "#2c1611",
    "editor.inactiveSelectionBackground": "#23100c",
    "editor.selectionHighlightBackground": "#3a1a13",
    "editor.wordHighlightBackground": "#3f1c14",
    "editor.wordHighlightStrongBackground": "#4a2117",
    "editor.findMatchBackground": "#e8b44c55",
    "editor.findMatchHighlightBackground": "#e8b44c33",
    "editor.findRangeHighlightBackground": "#e8b44c22",
    "editorBracketMatch.background": "#2a130f",
    "editorBracketMatch.border": "#e8b44c",
    "editorLineNumber.foreground": "#7d5541",
    "editorLineNumber.activeForeground": "#e8b44c",
    "editorIndentGuide.background": "#2f1a15",
    "editorIndentGuide.activeBackground": "#e8b44c55",
    "editorWhitespace.foreground": "#3b2019",
    "editorGutter.background": "#100705",
    "editorGutter.addedBackground": "#8cbf6a66",
    "editorGutter.modifiedBackground": "#e0a54c66",
    "editorGutter.deletedBackground": "#c2544a66",
    "activityBar.background": "#130b09",
    "activityBar.foreground": "#f4e6d1",
    "activityBarBadge.background": "#e8b44c",
    "activityBarBadge.foreground": "#120807",
    "sideBar.background": "#120907",
    "sideBar.foreground": "#f4e6d1",
    "sideBarSectionHeader.background": "#1a0e0b",
    "statusBar.background": "#130b09",
    "statusBar.foreground": "#f4e6d1",
    "statusBar.noFolderBackground": "#130b09",
    "statusBar.debuggingBackground": "#e8b44c",
    "statusBar.debuggingForeground": "#120807",
    "titleBar.activeBackground": "#130b09",
    "titleBar.activeForeground": "#f4e6d1",
    "titleBar.inactiveBackground": "#0f0806",
    "titleBar.inactiveForeground": "#c7a481",
    "panel.background": "#140a08",
    "panel.border": "#3a1b14",
    "panelTitle.activeForeground": "#f4e6d1",
    "panelTitle.inactiveForeground": "#c7a481",
    "badge.background": "#e8b44c",
    "badge.foreground": "#120807",
    "button.background": "#e8b44c",
    "button.foreground": "#120807",
    "button.hoverBackground": "#f0c161",
    "dropdown.background": "#1d100d",
    "dropdown.border": "#3a1b14",
    "input.background": "#140b09",
    "input.foreground": "#f4e6d1",
    "input.placeholderForeground": "#a38267",
    "list.activeSelectionBackground": "#24130f",
    "list.activeSelectionForeground": "#f4e6d1",
    "list.inactiveSelectionBackground": "#1c0f0c",
    "list.hoverBackground": "#1c0f0c",
    "list.highlightForeground": "#e8b44c",
    "editorGroupHeader.tabsBackground": "#140a08",
    "tab.activeBackground": "#1a0e0b",
    "tab.inactiveBackground": "#140a08",
    "tab.activeForeground": "#f4e6d1",
    "tab.inactiveForeground": "#c7a481",
    "scrollbarSlider.background": "#e8b44c33",
    "scrollbarSlider.hoverBackground": "#e8b44c55",
    "scrollbarSlider.activeBackground": "#e8b44c77",
    "peekView.border": "#e8b44c",
    "peekViewEditor.background": "#140a08",
    "peekViewEditor.matchHighlightBackground": "#e8b44c33",
    "peekViewResult.background": "#120907",
    "peekViewResult.matchHighlightBackground": "#e8b44c33",
    "terminal.background": "#100705",
    "terminal.foreground": "#f4e6d1",
    "terminalCursor.foreground": "#e8b44c",
    "terminal.ansiBlack": "#100705",
    "terminal.ansiRed": "#c2544a",
    "terminal.ansiGreen": "#86b971",
    "terminal.ansiYellow": "#e8b44c",
    "terminal.ansiBlue": "#d4834d",
    "terminal.ansiMagenta": "#c86a63",
    "terminal.ansiCyan": "#e0c99e",
    "terminal.ansiWhite": "#f4e6d1",
    "terminal.ansiBrightBlack": "#2f1a15",
    "terminal.ansiBrightRed": "#d86a5f",
    "terminal.ansiBrightGreen": "#9bcb84",
    "terminal.ansiBrightYellow": "#f0c161",
    "terminal.ansiBrightBlue": "#e09963",
    "terminal.ansiBrightMagenta": "#d88074",
    "terminal.ansiBrightCyan": "#ead6a9",
    "terminal.ansiBrightWhite": "#fff4db"
  },
  "tokenColors": [
    {
      "scope": [
        "source",
        "text"
      ],
      "settings": {
        "foreground": "#ff0000",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#b38266",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#ff4d4d"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#ff7f66"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "number"
      ],
      "settings": {
        "foreground": "#ffb347"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#f9c642"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#ffd25f"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "storage.type.cs",
        "support.type"
      ],
      "settings": {
        "foreground": "#f58634"
      }
    },
    {
      "scope": [
        "variable",
        "meta.definition.variable",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.property-name"
      ],
      "settings": {
        "foreground": "#f2d8a2"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#ff7f66"
      }
    },
    {
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#ffffff",
        "background": "#ff4d4d"
      }
    }
  ]
}
